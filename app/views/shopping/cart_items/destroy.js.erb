$('.cart-count').html("<%= session_cart.shopping_cart_items.length-1%> Items")

$('#cart_<%= params[:variant_id]%>').fadeOut( 'slow', function(){ 
                  $(".log").text('Fade Out Transition Complete');
               });

<% if session_cart.shopping_cart_items.length-1 == 0 %>
	location.reload();
<%end%>