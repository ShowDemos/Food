<div class="container">
	

<h3>Orders</h3>

<div class="table">
  <table class="table table-condensed">
    <thead>
      <tr class='odd'>
        <th class='column1_header'>Number</th>
        <th>Email</th>
        <th>Completed at</th>
        <th>Shipped</th>
      </tr>
    </thead>

  <% @orders.each do |order| %>
    <tr class='<%= cycle("odd", "")%>'>
      <td><%= link_to order.number, admin_history_order_path(order) %></td>
      <td><%= order.email %></td>
      <td><%= order.completed_at %></td>
      <td>yes</td>
    </tr>
  <% end %>
  </table>
</div>
</div>